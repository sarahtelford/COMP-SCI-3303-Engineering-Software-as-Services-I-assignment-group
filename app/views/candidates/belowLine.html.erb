<h1 class="ui header">
  Voting Below The Line
</h1>

<br>

<table class="ui table">
  <tbody>
  <% @candidate.each do |candidate| %>
    <tr>
      <td><%= candidate.name %></td>
      <td><%= candidate.party %></td>
      <td><%= candidate.birthday %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<br>
<%= link_to root_path, class: "ui basic button" do %>
  Back
<% end %>